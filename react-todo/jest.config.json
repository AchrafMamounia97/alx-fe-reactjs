{
  "testEnvironment": "jsdom",
  "moduleFileExtensions": ["js", "jsx"],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js?(x)"],
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "setupFilesAfterEnv": ["./src/setupTests.js"]
}
